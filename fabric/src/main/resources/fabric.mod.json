{
  "schemaVersion": 1,
  "id": "music_hud",
  "version": "${version}",
  "name": "Music Hud",
  "description": "A mod allow to play music from NCM in whole server ",
  "authors": [
    "Etern"
  ],
  "contact": {
    "sources": "https://github.com/Etern-34520/MusicHud"
  },
  "license": "LGPL-3.0",
  "icon": "assets/music_hud/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "indi.etern.musichud.fabric.CommonInitializer"
    ],
    "client": [
      "indi.etern.musichud.fabric.client.ClientInitializer",
      "indi.etern.musichud.fabric.config.ConfigRegister"
    ],
    "server": [
      "indi.etern.musichud.fabric.config.ConfigRegister"
    ],
    "modmenu": [
      "indi.etern.musichud.fabric.config.ModMenuConfigurer"
    ]
  },
  "mixins": [
    "music_hud.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.17.3",
    "minecraft": "~1.21.8",
    "java": ">=21",
    "architectury": ">=17.0.8",
    "fabric-api": "*",
    "modmenu": "*",
    "modernui": "*",
    "forgeconfigapiport": "*"
  },
  "suggests": {
    "another-mod": "*"
  }
}
